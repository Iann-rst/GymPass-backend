  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/bcryptjs": "3.0.0",
    "@types/node": "24.0.8",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-c8": "0.33.0",
    "@vitest/ui": "3.2.4",
    "eslint": "9.30.0",
    "npm-run-all": "4.1.5",
    "prisma": "6.11.0",
    "supertest": "7.1.1",
    "tsup": "8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
