  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/bcryptjs": "3.0.0",
    "@types/node": "25.3.0",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-c8": "0.33.0",
    "@vitest/ui": "4.0.18",
    "eslint": "10.0.1",
    "npm-run-all": "4.1.5",
    "prisma": "7.4.1",
    "supertest": "7.2.2",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.1.1",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
